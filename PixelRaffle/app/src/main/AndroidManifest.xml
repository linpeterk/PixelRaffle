<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.revature.pixelraffle">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PixelRaffle">
        <activity
            android:name="com.revature.pixelraffle.ui.screens.adama.ui.UserActivityHistories"
            android:exported="false"
            android:label="@string/title_activity_user_histories"
            android:theme="@style/Theme.PixelRaffle" />
        <activity
            android:name="com.revature.pixelraffle.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.PixelRaffle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>